<?xml version="1.0" encoding="UTF-8"?>
<server description="new server">

    <!-- Enable features -->
    <featureManager>
        <feature>jaxrs-2.0</feature>
        <feature>usr:consulDiscovery</feature>
		<!-- useful for debugging the Consul user feature
		<feature>osgiConsole-1.0</feature>
		 -->
